# General
# ------------------------------------

alias cat=bat
alias cd-="cd -"
alias lg=lazygit
alias v=vim

# Git
# -----------------------------------

alias g=git

# LSDeluxe
# ------------------------------------

alias l="lsd"
alias la="lsd -A"
alias ll="lsd -l"
alias lla="lsd -Al"
alias lt="lsd --tree"

# Yarn
# ------------------------------------

alias yl="yarn link"
alias yul="yarn unlink; yarn link"
alias yst="yarn start"

